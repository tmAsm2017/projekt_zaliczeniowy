<?xml version='1.0' encoding='utf-8'?>
<widget xmlns="http://www.w3.org/ns/widgets"
        xmlns:gap="http://phonegap.com/ns/1.0"
        id="projekt_zal"
        version="1.0.0">
    <name>Gdzie zaparkowałem samochów</name>
    <description>Aplikacja ułatwia zlokalizowanie pojazdu na wielkich parkingach centr handlowych</description>
    <author href="//github.com/tmAsm2017" email="tomasz.maj@microsoft.wsei.edu.pl">
        Tomasz Maj
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />

    <plugin name="cordova-plugin-device" spec="~1.1.4"/>
    <plugin name="cordova-plugin-dialogs" spec="~1.3.1"/>
    <plugin name="cordova-plugin-geolocation" spec="~2.4.2"/>
    <plugin name="cordova-plugin-camera" spec="~2.4.0"/>
    <splash src="img/splash.png"/>
    <icon src="img/icon.png"/>
</widget>
